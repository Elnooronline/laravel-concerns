{
    "name": "elnooronline/laravel-concerns",
    "description": "The components that make up the Elnooronline's Laravel Projects",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Fathy",
            "email": "aliraqi.dev@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.6.*|5.7.*",
        "laracasts/flash": "^3.0",
        "laracasts/presenter": "^0.2.1",
        "elnooronline/laravel-locales": "^1.0",
        "ezyang/htmlpurifier": "^4.10",
        "spatie/laravel-medialibrary": "^7.5",
        "elnooronline/laravel-bootstrap-forms": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Elnooronline\\LaravelConcerns\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Elnooronline\\LaravelConcerns\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elnooronline\\LaravelConcerns\\Providers\\ServiceProvider",
                "Elnooronline\\LaravelConcerns\\Providers\\ValidationServiceProvider"
            ],
            "aliases": {

            }
        }
    }
}
